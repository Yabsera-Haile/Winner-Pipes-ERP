<odoo>

<data>
    <record id="cms_problem_setup_form" model="ir.ui.view">
        <field name="name">Cms problem setup form</field>
        <field name="model">cms.problem.setup</field>
        <field name="arch" type="xml">
            <form string="Cms Problem Setup">
                <sheet>
                    <group>
                        <field name="sr_type"/>
                        <field name="problem_type"/>
                        <field name="resolution_type"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

     <record id="cms_problem_setup_tree" model="ir.ui.view">
        <field name="name">Cms problem setup list</field>
        <field name="model">cms.problem.setup</field>
        <field name="arch" type="xml">
            <tree>
                <field name="sr_type"/>
                <field name="problem_type"/>
                <field name="resolution_type"/>
                <field name="active"/>
            </tree>
        </field>
    </record>


</data>
</odoo>