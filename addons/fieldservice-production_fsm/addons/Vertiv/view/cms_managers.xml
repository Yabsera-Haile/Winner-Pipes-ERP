<odoo>

<data>
    <record id="cms_manager_form" model="ir.ui.view">
        <field name="name">Cms Managers form</field>
        <field name="model">cms.managers</field>
        <field name="arch" type="xml">
            <form string="Cms Managers">
                <sheet>
                    <group>
                        <field name="group_name"/>
                        <field name="resource_name"/>
                        <field name="resource_number"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

     <record id="cms_manager_tree" model="ir.ui.view">
        <field name="name">Cms Managers list</field>
        <field name="model">cms.managers</field>
        <field name="arch" type="xml">
            <tree string="Csm Manager">
                <field name="group_name"/>
                <field name="resource_name"/>
                <field name="resource_number"/>
            </tree>
        </field>
    </record>


</data>
</odoo>