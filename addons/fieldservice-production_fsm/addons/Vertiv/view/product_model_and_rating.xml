<odoo>

<data>
    <record id="product_model_form" model="ir.ui.view">
        <field name="name">Product model form</field>
        <field name="model">product.model</field>
        <field name="arch" type="xml">
            <form string="Product Model">
                <sheet>
                    <group>
                        <field name="model"/>
                        <field name="product_group"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="product_model_tree" model="ir.ui.view">
        <field name="name">Product model tree</field>
        <field name="model">product.model</field>
        <field name="arch" type="xml">
            <tree>
                <field name="model"/>
                <field name="product_group"/>
            </tree>
        </field>
    </record>

    <record id="product_rating_form" model="ir.ui.view">
        <field name="name">Product rating form</field>
        <field name="model">product.rating</field>
        <field name="arch" type="xml">
            <form string="Product Model">
                <sheet>
                    <group>
                        <field name="model"/>
                        <field name="rating"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="product_rating_tree" model="ir.ui.view">
        <field name="name">Product rating tree</field>
        <field name="model">product.rating</field>
        <field name="arch" type="xml">
            <tree>
                <field name="model"/>
                <field name="rating"/>
            </tree>
        </field>
    </record>

